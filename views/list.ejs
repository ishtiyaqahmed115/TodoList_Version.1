                    <p name = "item"><%= newItems[i]; %></p> 
                </div>
                <!-- <button formaction="/remove" type="submit"> Remove </button> -->
            <% } %> 
            <form class = "item" action="/" method="POST">
                <input type="text" name="item" placeholder="Add Item" autocomplete="off">
                <button type="submit" name = "list" value=<%= typeOfList %> >+</button>
            </form>
            <script src="" async defer></script>
        </div>
    </body>
